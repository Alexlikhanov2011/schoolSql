SELECT faculty.id, faculty.name, faculty.color, student.name from faculty, student
where student.faculty_id = faculty.id and faculty_id = 4;

SELECT student.id, student.name, faculty.name from student, faculty
where student.faculty_id = faculty_id and student.id = 10;

